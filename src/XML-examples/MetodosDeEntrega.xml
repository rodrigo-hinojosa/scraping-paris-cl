            <custom-attribute attribute-id="scheduledShipping">false</custom-attribute>
            <custom-attribute attribute-id="storePickupEnabled">false</custom-attribute>
            <custom-attribute attribute-id="summaryRenderingTemplate">checkout/shipping/inc/address/miniaddress.isml</custom-attribute>
