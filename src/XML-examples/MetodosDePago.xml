<?xml version="1.0" encoding="UTF-8"?>
<payment-settings xmlns="http://www.demandware.com/xml/impex/paymentsettings/2009-09-15">
    <payment-method method-id="GIFT_CERTIFICATE">
        <name xml:lang="x-default">Gift Certificate</name>
        <enabled-flag>false</enabled-flag>
        <processor-id>BASIC_GIFT_CERTIFICATE</processor-id>
        <custom-attributes>
            <custom-attribute attribute-id="isBicEnabled">false</custom-attribute>
            <custom-attribute attribute-id="isSupportedBankListRequired">false</custom-attribute>
        </custom-attributes>
    </payment-method>

    <payment-method method-id="BANK_TRANSFER">
        <name xml:lang="x-default">Bank Transfer</name>
        <enabled-flag>false</enabled-flag>
    </payment-method>

    <payment-method method-id="BML">
        <name xml:lang="x-default">Bill Me Later</name>
        <enabled-flag>false</enabled-flag>
        <processor-id>CYBERSOURCE_BML</processor-id>
    </payment-method>

    <payment-method method-id="DW_APPLE_PAY">
        <name xml:lang="x-default">Apple Pay</name>
        <enabled-flag>false</enabled-flag>
    </payment-method>

    <payment-method method-id="DW_ANDROID_PAY">
        <name xml:lang="x-default">Android Pay</name>
        <enabled-flag>false</enabled-flag>
    </payment-method>

    <payment-method method-id="GIFT_CARD">
        <name xml:lang="x-default">Gift Card</name>
        <enabled-flag>false</enabled-flag>
        <processor-id>GIFTCARD</processor-id>
        <custom-attributes>
            <custom-attribute attribute-id="isBicEnabled">false</custom-attribute>
            <custom-attribute attribute-id="isSupportedBankListRequired">false</custom-attribute>
        </custom-attributes>
    </payment-method>

    <payment-method method-id="TRANSBANK">
        <name xml:lang="x-default">Trans Bank</name>
        <enabled-flag>false</enabled-flag>
        <processor-id>CYBERSOURCE_CREDIT</processor-id>
    </payment-method>

    <payment-method method-id="CENCOSUD_CARD">
        <name xml:lang="x-default">Cencosud Card</name>
        <description xml:lang="x-default">Visa or Master</description>
        <enabled-flag>true</enabled-flag>
        <processor-id>CYBERSOURCE_CREDIT</processor-id>
        <custom-attributes>
            <custom-attribute attribute-id="template" xml:lang="x-default">cybersource.isml</custom-attribute>
        </custom-attributes>
    </payment-method>

    <payment-method method-id="XCash">
        <name xml:lang="x-default">Cencosud Card</name>
        <enabled-flag>true</enabled-flag>
        <processor-id>XCash</processor-id>
        <custom-attributes>
            <custom-attribute attribute-id="template" xml:lang="x-default">catcerrada.isml</custom-attribute>
        </custom-attributes>
    </payment-method>

    <payment-method method-id="CREDIT_CARD">
        <name xml:lang="x-default">Credit Card</name>
        <enabled-flag>true</enabled-flag>
        <processor-id>CYBERSOURCE_CREDIT</processor-id>
        <custom-attributes>
            <custom-attribute attribute-id="isBicEnabled">false</custom-attribute>
            <custom-attribute attribute-id="isSupportedBankListRequired">false</custom-attribute>
            <custom-attribute attribute-id="merchantID" xml:lang="x-default">cencosud</custom-attribute>
            <custom-attribute attribute-id="merchantKey" xml:lang="x-default"></custom-attribute>
            <custom-attribute attribute-id="template" xml:lang="x-default">cybersource.isml</custom-attribute>
        </custom-attributes>
    </payment-method>

    <payment-method method-id="GIFTCARD">
        <name xml:lang="x-default">Gift Card</name>
        <enabled-flag>true</enabled-flag>
        <processor-id>GIFT_CARD</processor-id>
        <custom-attributes>
            <custom-attribute attribute-id="template" xml:lang="x-default">giftcard.isml</custom-attribute>
        </custom-attributes>
    </payment-method>

    <payment-method method-id="WEBPAY">
        <name xml:lang="x-default">Redcompra</name>
        <enabled-flag>true</enabled-flag>
        <processor-id>WEBPAY</processor-id>
        <custom-attributes>
            <custom-attribute attribute-id="template" xml:lang="x-default">webpay.isml</custom-attribute>
        </custom-attributes>
    </payment-method>

    <payment-card card-type="Master">
        <name xml:lang="x-default">Master Card</name>
        <enabled-flag>false</enabled-flag>
        <card-number-settings>
            <enable-checksum-verification>true</enable-checksum-verification>
            <number-length>
                <length>16</length>
            </number-length>
            <number-prefix>
                <prefix>2221-2720</prefix>
                <prefix>51-55</prefix>
            </number-prefix>
        </card-number-settings>
        <security-code-length>3</security-code-length>
    </payment-card>

    <payment-card card-type="Discover">
        <name xml:lang="x-default">Discover</name>
        <enabled-flag>false</enabled-flag>
        <card-number-settings>
            <enable-checksum-verification>true</enable-checksum-verification>
            <number-length>
                <length>16</length>
            </number-length>
            <number-prefix>
                <prefix>6011</prefix>
                <prefix>622126-622925</prefix>
                <prefix>644-649</prefix>
                <prefix>65</prefix>
            </number-prefix>
        </card-number-settings>
        <security-code-length>3</security-code-length>
        <countries>
            <country country-code="US"/>
        </countries>
    </payment-card>

    <payment-card card-type="Visa">
        <name xml:lang="x-default">Visa</name>
        <enabled-flag>true</enabled-flag>
        <card-number-settings>
            <enable-checksum-verification>true</enable-checksum-verification>
            <number-length>
                <length>13</length>
                <length>16</length>
            </number-length>
            <number-prefix>
                <prefix>4</prefix>
            </number-prefix>
        </card-number-settings>
        <security-code-length>3</security-code-length>
    </payment-card>

    <payment-card card-type="Amex">
        <name xml:lang="x-default">American Express</name>
        <enabled-flag>true</enabled-flag>
        <card-number-settings>
            <enable-checksum-verification>true</enable-checksum-verification>
            <number-length>
                <length>15</length>
            </number-length>
            <number-prefix>
                <prefix>34</prefix>
                <prefix>37</prefix>
            </number-prefix>
        </card-number-settings>
        <security-code-length>4</security-code-length>
    </payment-card>

    <payment-card card-type="MasterCard">
        <name xml:lang="x-default">MasterCard</name>
        <enabled-flag>true</enabled-flag>
        <card-number-settings>
            <enable-checksum-verification>true</enable-checksum-verification>
            <number-length>
                <length>16</length>
            </number-length>
            <number-prefix>
                <prefix>51-55</prefix>
            </number-prefix>
        </card-number-settings>
        <security-code-length>3</security-code-length>
        <custom-attributes>
            <custom-attribute attribute-id="csEnablePayerAuthentication">false</custom-attribute>
        </custom-attributes>
    </payment-card>

</payment-settings>
